# Required for Supabase client (browser and server)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Prefer server-side vars when available (optional, fallback used if unset)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Comma-separated list of superadmin emails allowed to access /superadmin
# Example: admin1@example.com,admin2@example.com
SUPERADMIN_EMAILS=

# Analytics/monitoring (optional)
NEXT_PUBLIC_VERCEL_ANALYTICS=1
SENTRY_DSN=

# Node environment
NODE_ENV=development

